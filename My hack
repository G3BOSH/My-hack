    -- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ¾Ñ‚Ğ¿Ñ€Ğ°Ğ²ĞºĞ¸ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹
    local function sendMessages()
        local message = MessageBox.Text .. (censorshipEnabled and "" or " le le le le")
        local count = tonumber(CountBox.Text)

        if message ~= "" and count then
            for i = 1, count do
                local chatChannel = TextChatService:WaitForChild("TextChannels"):WaitForChild("RBXGeneral")
                chatChannel:SendAsync(message)
                wait(1) -- Ğ—Ğ°Ğ´ĞµÑ€Ğ¶ĞºĞ° Ğ² 1 ÑĞµĞºÑƒĞ½Ğ´Ñƒ Ğ¼ĞµĞ¶Ğ´Ñƒ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸ÑĞ¼Ğ¸
            end
        else
            warn("ĞŸĞ¾Ğ¶Ğ°Ğ»ÑƒĞ¹ÑÑ‚Ğ°, Ğ²Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ĞºĞ¾Ñ€Ñ€ĞµĞºÑ‚Ğ½Ğ¾Ğµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ Ğ¸ ĞºĞ¾Ğ»Ğ¸Ñ‡ĞµÑÑ‚Ğ²Ğ¾.")
        end
    end

    SendButton.MouseButton1Click:Connect(sendMessages)

    -- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ ÑĞ²Ğ¾Ñ€Ğ°Ñ‡Ğ¸Ğ²Ğ°Ğ½Ğ¸Ñ Ğ¸ Ñ€Ğ°Ğ·Ğ²Ğ¾Ñ€Ğ°Ñ‡Ğ¸Ğ²Ğ°Ğ½Ğ¸Ñ GUI
