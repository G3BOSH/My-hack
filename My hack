    -- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ Ğ¿ĞµÑ€ĞµĞºĞ»ÑÑ‡ĞµĞ½Ğ¸Ñ Ñ†ĞµĞ½Ğ·ÑƒÑ€Ñ‹
    CensorshipToggle.MouseButton1Click:Connect(function()
        censorshipEnabled = not censorshipEnabled
        CensorshipToggle.Text = censorshipEnabled and "Ğ¦ĞµĞ½Ğ·ÑƒÑ€Ğ° Ğ²ĞºĞ»ÑÑ‡ĞµĞ½Ğ°" or "Ğ¦ĞµĞ½Ğ·ÑƒÑ€Ğ° Ğ¾Ñ‚ĞºĞ»ÑÑ‡ĞµĞ½Ğ°"
    end)

    -- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ ÑĞ¼ĞµĞ½Ñ‹ ÑĞ·Ñ‹ĞºĞ°
    LanguageButton.MouseButton1Click:Connect(function()
        currentLanguage = (currentLanguage == "ru") and "en" or "ru"
        MessageBox.PlaceholderText = (currentLanguage == "ru") and "Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğµ" or "Enter message"
        CountBox.PlaceholderText = (currentLanguage == "ru") and "Ğ’Ğ²ĞµĞ´Ğ¸Ñ‚Ğµ ĞºĞ¾Ğ»Ğ¸Ñ‡ĞµÑÑ‚Ğ²Ğ¾" or "Enter count"SendButton.Text = (currentLanguage == "ru") and "ĞÑ‚Ğ¿Ñ€Ğ°Ğ²Ğ¸Ñ‚ÑŒ" or "Send"
        ToggleButton.Text = (currentLanguage == "ru") and "Ğ¡Ğ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ/Ğ Ğ°Ğ·Ğ²ĞµÑ€Ğ½ÑƒÑ‚ÑŒ" or "Collapse/Expand"
        SettingsButton.Text = (currentLanguage == "ru") and "ĞĞ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸" or "Settings"
        LanguageButton.Text = (currentLanguage == "ru") and "Ğ˜Ğ·Ğ¼ĞµĞ½Ğ¸Ñ‚ÑŒ ÑĞ·Ñ‹Ğº" or "Change Language"
        CensorshipToggle.Text = censorshipEnabled and 
            ((currentLanguage == "ru") and "Ğ¦ĞµĞ½Ğ·ÑƒÑ€Ğ° Ğ²ĞºĞ»ÑÑ‡ĞµĞ½Ğ°" or "Censorship Enabled") or 
            ((currentLanguage == "ru") and "Ğ¦ĞµĞ½Ğ·ÑƒÑ€Ğ° Ğ¾Ñ‚ĞºĞ»ÑÑ‡ĞµĞ½Ğ°" or "Censorship Disabled")
    end)

    print("Ğ¡Ğ¾Ğ·Ğ´Ğ°Ğ½ GUI Ğ´Ğ»Ñ Ğ¸Ğ³Ñ€Ğ¾ĞºĞ°: " .. player.Name)
end

-- Ğ¡Ğ¾Ğ·Ğ´Ğ°Ğ½Ğ¸Ğµ GUI Ğ´Ğ»Ñ ĞºĞ°Ğ¶Ğ´Ğ¾Ğ³Ğ¾ Ğ¸Ğ³Ñ€Ğ¾ĞºĞ°
Players.PlayerAdded:Connect(function(player)
    createGui(player)
end)

-- Ğ¡Ğ¾Ğ·Ğ´Ğ°Ğ½Ğ¸Ğµ GUI Ğ´Ğ»Ñ ÑƒĞ¶Ğµ ÑÑƒÑ‰ĞµÑÑ‚Ğ²ÑƒÑÑ‰Ğ¸Ñ… Ğ¸Ğ³Ñ€Ğ¾ĞºĞ¾Ğ²
for _, player in ipairs(Players:GetPlayers()) do
    createGui(player)
end

-- ĞĞ±ĞµÑĞ¿ĞµÑ‡ĞµĞ½Ğ¸Ğµ Ğ½ĞµĞ¿Ñ€ĞµÑ€Ñ‹Ğ²Ğ½Ğ¾Ğ¹ Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‹ ÑĞºÑ€Ğ¸Ğ¿Ñ‚Ğ°
